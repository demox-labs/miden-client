    "classnames": "^2.5.1",
    "dexie-react-hooks": "^1.1.7",
    "framer-motion": "^11.2.0",
    "jotai": "^2.8.0",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "overlayscrollbars": "^2.8.0",
    "overlayscrollbars-react": "^0.5.6",
    "react": "^18",
    "react-dom": "^18",
    "react-use": "^17.5.0",
    "swiper": "^11.1.3",
    "wasm": "file:../wasm/pkg"
  },
  "devDependencies": {
